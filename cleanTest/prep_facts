(define (problem foodprep)
    (:domain foodprepworld)
    (:objects lettuce kale spinach tomato carrots cucumber seeds myhand kitchensink kitchencounter woodbowl woodboard chefknife pumpkin-seeds croutons chopsticks)
    (:init (vegetable lettuce)
            (vegetable kale)
            (vegetable spinach)
            (leafy lettuce)
            (leafy kale)
            (leafy spinach)
            (vegetable tomato)
            (vegetable carrots)
            (vegetable cucumber)
            (hand myhand)
            (surface kitchencounter)
            (surface woodboard)
            (sink kitchensink)
            (on lettuce kitchencounter)
            (on kale kitchencounter)
            (on spinach kitchencounter)
            (on tomato kitchencounter)
            (on carrots kitchencounter)
            (on cucumber kitchencounter)
            (empty myhand)
            (bowl woodbowl)
            (cuttingboard woodboard)
            (knife chefknife)
            (on chefknife kitchencounter)
            (crunchy pumpkin-seeds)
            (crunchy croutons)
            (on pumpkin-seeds kitchencounter)
            (on croutons kitchencounter)
            (mixer chopsticks)
            (on chopsticks kitchencounter))
    (:goal (salad_ready_in woodbowl))
)