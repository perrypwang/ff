(define (problem make-salad)
	(:domain salad-world)
	(:objects lettuce spinach kale carrot cucumber crouton pumpkin-seeds ranch balsamic
				chef-knife metal-chopstick wood-bowl wood-board countertop kitchen-sink)
	(:init 
		(leafy lettuce)
		(leafy spinach)
		(leafy kale)
		(vegetable lettuce)
		(vegetable spinach)
		(vegetable kale)
		(vegetable carrot)
		(vegetable cucumber)
		(crunchy crouton)
		(crunchy pumpkin-seeds)
		(dressing ranch)
		(dressing balsamic)
		(knife chef-knife)
		(chopstick metal-chopstick)
		(bowl wood-bowl)
		(cutting-board wood-board)
		(table countertop)
		(sink kitchen-sink)
		(hand_empty)
		(on_or_in lettuce countertop)
		(on_or_in spinach countertop)
		(on_or_in kale countertop)
		(on_or_in carrot countertop)
		(on_or_in cucumber countertop)
		(on_or_in crouton countertop)
		(on_or_in pumpkin-seeds countertop)
		(on_or_in ranch countertop)
		(on_or_in balsamic countertop)
		(on_or_in chef-knife countertop)
		(on_or_in metal-chopstick countertop)
		(on_or_in wood-bowl countertop)
		(on_or_in wood-board countertop)
	)
	(:goal salad_ready)
)
